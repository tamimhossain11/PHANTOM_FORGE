# Phantom Forge - Environment Variables Template
# Team Phantom Forge | Dreams of Bangladesh
# Copy this file to .env and update the values as needed

# Development Configuration
NODE_ENV=development
VITE_APP_NAME="Phantom Forge - Advanced Robotics"
VITE_APP_VERSION=1.0.0

# Application URLs
VITE_APP_URL=http://localhost:3001
VITE_API_BASE_URL=http://localhost:3001/api

# Team Information
VITE_TEAM_NAME="Team Phantom Forge"
VITE_ORGANIZATION="Dreams of Bangladesh"
VITE_COMPETITION="7th World Invention Competition and Exhibition"

# Robot Configuration
VITE_DRONE_NAME="Diganta Prohori"
VITE_AUV_NAME="HydroJan 0.4"
VITE_ROV_NAME="Dingi"

# Project Details
VITE_PROJECT_TITLE="Smart Flood Management System"
VITE_PROJECT_DESCRIPTION="Next-generation flood management robotic vehicles for human rescue and water resources management"

# GitHub Repository
VITE_GITHUB_REPO="https://github.com/tamimhossain11/PHANTOM_FORGE.git"
VITE_GITHUB_USERNAME="tamimhossain11"

# Contact Information
VITE_CONTACT_EMAIL="team@phantomforge.bd"
VITE_LOCATION="Bangladesh"

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_TELEMETRY_SIMULATION=true
VITE_ENABLE_REAL_TIME_DATA=true
VITE_ENABLE_GLASS_EFFECTS=true

# Performance Settings
VITE_ANIMATION_DURATION=600
VITE_DATA_UPDATE_INTERVAL=1000

# Build Configuration
VITE_BUILD_TARGET=es2015
VITE_LEGACY_SUPPORT=false

# Deployment Settings (Update for production)
# VITE_APP_URL=https://your-domain.com
# VITE_API_BASE_URL=https://api.your-domain.com
